#: venv/lib/python3.8/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Расширенные настройки"

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Простые страницы"

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:9
#: venv/lib/python3.8/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:12
#, fuzzy
#| msgid ""
#| "Example: '/about/contact/'. Make sure to have leading and trailing "
#| "slashes."
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Пример: '/about/contact/'. Убедитесь, что ввели начальную и конечную косые "
"черты."

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Значение должно состоять только из букв, цифр и символов точки, "
"подчеркивания, тире, косой черты и тильды."

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:29
#, fuzzy
#| msgid "Example: '/about/contact'. Make sure to have a leading slash."
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Например: '/about/contact'. Убедитесь, что в начале адреса находится косая "
"черта."

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:42
msgid "URL is missing a leading slash."
msgstr "В начале URL отсутствует косая черта"

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a trailing slash."
msgstr "В конце URL отсутствует косая черта"

#: venv/lib/python3.8/site-packages/django/contrib/flatpages/forms.py:64
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Простая страница с адресом %(url)s уже существует для сайта %(site)s"

